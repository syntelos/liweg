 */
liweg display(){
    data fb uint;
    data x uint;
    data y uint;
    fb = 0;
    x = 0;
    y = register_x;
    blit( fb, x, y, register);
}

liweg main(){
    framebuffers(1);
    data cc uint;
    array glyph8 type:4 8;
    glyph8 = {
        0b0110,
        0b1001,
        0b1001,
        0b1001,
        0b1001,
        0b1001,
        0b1001,
        0b0110
    };

    eval(glyph8, display);

    show(0);
}
